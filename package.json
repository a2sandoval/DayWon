{
  "name": "juggernaut",
  "version": "1.0.0",
  "description": "Developing a application for powerlifting measurements based on the juggernaut method",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakedykstra/Juggernaut.git"
  },
  "author": "Jacob Dykstra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jakedykstra/Juggernaut/issues"
  },
  "homepage": "https://github.com/jakedykstra/Juggernaut#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "chart.js": "^2.7.3",
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "create-react-app": "^1.5.2",
    "dotenv": "^6.0.0",
    "express": "^4.18.2",
    "express-graphql": "^0.7.1",
    "express-session": "^1.11.3",
    "graphql": "^14.0.2",
    "gravatar": "^1.6.0",
    "localtunnel": "^1.8.3",
    "lodash": "^4.17.11",
    "mongoose": "^4.11.1",
    "morgan": "*",
    "nodemon": "^1.11.0",
    "path-parser": "^2.0.2",
    "react-chartjs-2": "^2.7.4",
    "validator": "^9.4.1"
  }
}
